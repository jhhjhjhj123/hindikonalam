    <h1>Middleware Example</h1>
    <p>This is a simple HTML page demonstrating the concept of middleware.</p>

    <!-- Your middleware script -->
    <script>
        // Example middleware function
        function middlewareFunction(event) {
            // Add your middleware logic here
            console.log("Middleware executed");
        }

        // Attach middleware to an event (e.g., click event)
        document.addEventListener('click', middlewareFunction);
    </script>
</body>
